{
  "ApiSettings": {
    "AppUrl": "http://localhost:8700",
    "JsonSerializer": "Snakecase",
    "PathPrefix": "api/{version}",
    "Domain": "ObservabilityDemo",
    "Application": "ApiOne",
    "Version": "v1",
    "BuildVersion": "1.0.0",
    "SupportedCultures": [ "en-US", "pt-BR", "es-ES" ],
    "RequestKeyProperty": "RequestKey",
    "TimezoneHeader": "Timezone",
    "TimezoneDefault": "UTC",
    "TimeElapsedProperty": "X-Internal-Time"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
